{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "semicolon": false,
    "trailing-comma": false,
    "jsx-wrap-multiline": false,
    "jsx-no-multiline-js": false,
    "member-access": [
      false
    ],
    "ordered-imports": [
      true
    ],
    "no-console": [
      false
    ],
    "no-var-requires": true,
    "quotemark": [
      true,
      "double",
      "jsx-double"
    ]
  },
  "rulesDirectory": []
}